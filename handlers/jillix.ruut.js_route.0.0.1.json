{
    "name": "jillix.ruut.js_route",
    "version": "0.0.1",
    "description": "Get route value and parameters from a path.",
    "input": {
        "data": {
            "path": "<URLPath>"
        },
        "state": {
            "config": "<Ruut.js#config>",
            "router": "<Ruut.js#instance>"
        }
    },
    "output": {
        "data": {
            "route": "<Ruut.js#result>",
            "params": "<Ruut.js#params>"
        }
    }
}
