  {
    "name": "login",
    "version": "0.0.7",
    "description": "jillix engine login module",
    "main": "server/login.js",
    "repository": {
      "type": "git",
      "url": "git://github.com/adioo/login.git"
    },
    "bugs": {
      "url": "https://github.com/adioo/login/issues"
    },
    "homepage": "https://github.com/adioo/login",
    "composition": {
      "flow": [
        [
          "loginUser",
          "loginUser"
        ],
        [
          "logoutUser",
          "logoutUser"
        ],
        [
          "signupUser",
          "signupUser"
        ],
        [
          "resetPassword",
          "resetPassword"
        ]
      ],
      "client": {
        "module": [
          "login.js"
        ],
        "config": {},
        "flow": [
          [
            "loginUser",
            [
              ">flow",
              "@loginUser"
            ],
            [
              ":session",
              {
                "sid": "sid"
              }
            ],
            [
              ">flow",
              "userLoggedIn"
            ]
          ],
          [
            "logoutUser",
            [
              ":session",
              {
                "remove": true
              }
            ],
            [
              ">flow",
              "@logoutUser"
            ],
            [
              ">flow",
              "userLoggedOut"
            ]
          ],
          [
            "signupUser",
            [
              ">flow",
              "@signupUser"
            ],
            [
              ">flow",
              "userSignedUp"
            ]
          ],
          [
            "resetPassword",
            [
              ">flow",
              "@resetPassword"
            ],
            [
              ">flow",
              "passwordReseted"
            ]
          ]
        ]
      }
    },
    "readme": "ERROR: No README data found!",
    "_id": "login@0.0.7"
  },
